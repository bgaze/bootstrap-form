{
  "name": "bgaze/bootstrap-form",
  "description": "Bootstrap 4 forms builder for Laravel 10+",
  "keywords": [
    "laravel",
    "form",
    "bootstrap"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Bgaze",
      "email": "benjamin@bgaze.fr"
    }
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Bgaze\\BootstrapForm\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bgaze\\BootstrapForm\\BootstrapFormServiceProvider"
      ],
      "aliases": {
        "BF": "Bgaze\\BootstrapForm\\Support\\Facade",
        "Html": "Bgaze\\BootstrapForm\\Html\\Html"
      }
    }
  },
  "minimum-stability": "dev",
  "require-dev": {
    "laravel/pint": "dev-main"
  }
}
